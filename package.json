{
  "name": "minicde-bim-iso19650",
  "version": "1.0.0",
  "description": "A comprehensive CDE (Common Data Environment) system for BIM ISO 19650 compliance",
  "main": "backend/dist/index.js",
  "scripts": {
    "start": "node backend/dist/index.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "heroku-postbuild": "npm run build:frontend",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed",
    "db:generate": "cd backend && npm run db:generate",
    "test": "cd backend && npm test",
    "lint": "cd backend && npm run lint && cd ../frontend && npm run lint"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "keywords": [
    "bim",
    "iso19650",
    "cde",
    "construction",
    "project-management",
    "typescript",
    "react",
    "express"
  ],
  "author": "HoangLong24",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/minicde-bim-iso19650.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/minicde-bim-iso19650/issues"
  },
  "homepage": "https://qlda.hoanglong24.com",
  "dependencies": {
    "axios": "^1.11.0"
  }
}
